# A2A MINIMAL - Emergency Deploy
FROM python:3.11-slim

WORKDIR /app

# Install minimal dependencies
RUN pip install --no-cache-dir starlette uvicorn httpx

# Copy minimal server
COPY minimal_server.py ./

# Environment
ENV HOST=0.0.0.0
ENV A2A_PORT=10000

EXPOSE 10000

CMD ["python", "minimal_server.py"]
