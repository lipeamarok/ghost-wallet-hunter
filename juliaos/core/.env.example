# JuliaOS Environment Configuration

# Server Configuration
HOST="127.0.0.1"
PORT="8052"
HOST_URL=http://127.0.0.1:8052

# API Keys
OPENAI_API_KEY=your-openai-api-key
OPENAI_BASE_URL=some-base-url
OPENAI_MODEL=llama3-8b-8192
GROK_API_KEY=your-grok-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key
COINGECKO_API_KEY=your_coingecko_key
COINMARKETCAP_API_KEY=your_coinmarketcap_key
X_API_KEY=your-x-api-key
X_API_KEY_SECRET=your-x-api-key-secret
X_ACCESS_TOKEN=your-x-access-token
X_ACCESS_TOKEN_SECRET=your-x-access-token-secret

# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your-telegram-bot-token
TELEGRAM_MODERATOR_BOT_TOKEN=your-telegram-moderator-bot-token

# Blockchain RPC URLs
ETHEREUM_RPC_URL=https://mainnet.infura.io/v3/your_infura_key
ETHEREUM_TESTNET_RPC_URL=https://sepolia.infura.io/v3/your_infura_key
POLYGON_RPC_URL=https://polygon-rpc.com
POLYGON_TESTNET_RPC_URL=https://mumbai.polygon.io
ARBITRUM_RPC_URL=https://arb1.arbitrum.io/rpc
OPTIMISM_RPC_URL=https://mainnet.optimism.io
BASE_RPC_URL=https://mainnet.base.org
AVALANCHE_RPC_URL=https://api.avax.network/ext/bc/C/rpc
BSC_RPC_URL=https://bsc-dataseed.binance.org
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
FANTOM_RPC_URL=https://rpc.ftm.tools

# Database Configuration
DB_HOST="localhost"
DB_USER="postgres"
DB_PASSWORD="postgres"
DB_NAME="postgres"
DB_PORT="5435"
DB_PATH=~/.juliaos/juliaos.sqlite

# Redis Configuration
REDIS_HOST="127.0.0.1"
REDIS_PORT="6379"
REDIS_PASSWORD=""

# Ceramic Network Configuration (Web3 Storage)
CERAMIC_NODE_URL=https://ceramic-clay.3boxlabs.com
CERAMIC_MAINNET_URL=https://ceramic.network

# IPFS Configuration
IPFS_API_URL=https://api.web3.storage
IPFS_API_KEY=your_web3_storage_key

# Julia Settings
JULIA_PATH=/usr/local/bin/julia
JULIA_HOST=127.0.0.1
JULIA_PORT=8053

# Trading Settings
DEFAULT_SLIPPAGE_TOLERANCE=0.5
GAS_PRICE_BUFFER=1.2
MAX_POSITION_SIZE=0.1
PRICE_VALIDATION_ENABLED=true
MAX_PRICE_DEVIATION=0.03

# Agent Settings
MAX_AGENTS_PER_SWARM=10
DEFAULT_COORDINATION_PROTOCOL=consensus
DEFAULT_DECISION_THRESHOLD=0.7
AGENT_UPDATE_INTERVAL=60000

# RPC Endpoints
ETH_RPC_URL=https://eth-mainnet.alchemyapi.io/v2/your-api-key
POLYGON_RPC_URL=https://polygon-mainnet.g.alchemy.com/v2/your-api-key
ARBITRUM_RPC_URL=https://arb-mainnet.g.alchemy.com/v2/your-api-key
OPTIMISM_RPC_URL=https://opt-mainnet.g.alchemy.com/v2/your-api-key
BASE_RPC_URL=https://base-mainnet.g.alchemy.com/v2/your-api-key

# Bridge Contract Addresses
ETH_BRIDGE_ADDRESS=0x...
POLYGON_BRIDGE_ADDRESS=0x...
ARBITRUM_BRIDGE_ADDRESS=0x...
OPTIMISM_BRIDGE_ADDRESS=0x...
BASE_BRIDGE_ADDRESS=0x...

# DEX Contract Addresses
UNISWAP_V3_FACTORY=0x...
UNISWAP_V3_ROUTER=0x...
SUSHISWAP_ROUTER=0x...

# API Keys (Optional)
ETHERSCAN_API_KEY=your-etherscan-api-key
POLYGONSCAN_API_KEY=your-polygonscan-api-key
ARBISCAN_API_KEY=your-arbiscan-api-key

# Monitoring (Optional)
PROMETHEUS_PORT=9090
GRAFANA_PORT=3000

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/juliaos.log

# Risk Management
MAX_DAILY_LOSS=0.05
MAX_DRAWDOWN=0.1

# Chainlink API Key
CHAINLINK_API_KEY=your-chainlink-api-key

JULIA_DEBUG=false